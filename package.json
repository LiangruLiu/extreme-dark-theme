{
	"name": "extreme-dark-theme",
	"displayName": "Extreme Dark Theme",
	"description": "A dark theme for VS Code made by LLR",
	"categories": ["Themes"],
	"keywords": ["vscode", "theme", "dark", "night", "black", "high-contrast", "AMOLED"],
	"publisher": "liangruliu",
	"icon": "images/icon.png",
	"version": "0.6.0",
	"engines": {
		"vscode": "^1.56.0"
	},
	"main": "./sources/extension",
	"activationEvents": [
		"onStartupFinished",
		"onCommand:extension.HiLLR"
	],
	"contributes": {
		"themes": [
			{
				"label": "Extreme Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/ExtremeDark.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Extreme Dark Theme",
			"properties": {
				"Extreme-Dark-Theme.Token Colors": {
					"type": "string",
					"default": "Original",
					"markdownDescription": "Under Development... \n\n Usage: \n\n 1. Use `Ctrl+Shift+P` to open `Command Palette` \n\n 2. Input `Developer: Reload Window` then execute \n\n 3. Repeat twice to apply changes",
					"enum": [
						"Original",
						"OneDarkPro"
					]
				}
			}
		},
		"commands": [
			{
				"command": "extension.HiLLR",
				"title": "HiLLR (hello world)"
			}
		]
	},
	"__metadata": {
		"id": "b9b4d88a-9d1d-415a-8168-aef2f4d3f526",
		"publisherDisplayName": "Liangru Liu",
		"publisherId": "38d3078a-e2e3-4217-8e44-6a8b6cf6faaf"
	}
}
